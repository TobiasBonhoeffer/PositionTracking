
{
    "elements": [
        {
            "type": "ValidationTextBox",
            "name": "APIKey",
            "caption": "API-Key"
        },
        {
            "type": "SelectVariable",
            "name": "SourceLatitude",
            "caption": "Tracker Latitude"
        },
        {
            "type": "SelectVariable",
            "name": "SourceLongitude",
            "caption": "Tracker Longitude"
        },
        {
            "type": "SelectVariable",
            "name": "HomeLatitude",
            "caption": "Home Latitude"
        },
        {
            "type": "SelectVariable",
            "name": "MapCenterLongitude",
            "caption": "Map Center Longitude"
        },
                {
            "type": "SelectVariable",
            "name": "MapCenterLatitude",
            "caption": "Map Center Latitude"
        },
        {
            "type": "SelectVariable",
            "name": "HomeLongitude",
            "caption": "Home Longitude"
        },
        {
            "type": "NumberSpinner",
            "name": "MapZoom",
            "caption": "Zoom Level",
            "minimum": 1,
            "maximum": 21
