        {
            "type": "ValidationTextBox",
            "name": "MapWidth",
            "caption": "Map (Width)"
        },
        {
            "type": "ValidationTextBox",
            "name": "MapHeight",
            "caption": "Map (Height)"
        },
        {
            "type": "SelectFile",
            "name": "HomeIcon",
            "caption": "Icon (Home)"
        },
        {
            "type": "SelectFile",
            "name": "TrackerIcon",
            "caption": "Icon (Tracker)"
        }
    ],
    "actions": [
        {
            "type": "SelectLocation",
            "caption": "Location",
            "name": "SimulatedLocation"
        },
        {
            "type": "Button",
            "caption": "Simulate Location",
            "onClick": "PST_SendLocation($id, $SimulatedLocation);"
        }
    ]
