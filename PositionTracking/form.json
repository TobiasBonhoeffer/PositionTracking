            "options": [
                { "caption": "Roadmap", "value": "roadmap" },
                { "caption": "Satellite", "value": "satellite" },
                { "caption": "Hybrid", "value": "hybrid" },
                { "caption": "Terrain", "value": "terrain" }
            ]
        },
        {
            "type": "NumberSpinner",
            "name": "UpdateLimit",
            "caption": "Limit Update",
            "suffix": "seconds"
